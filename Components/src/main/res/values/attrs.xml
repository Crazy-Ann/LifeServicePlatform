<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TabLayout">
        <attr name="tl_indicatorAlwaysInCenter" format="boolean"/>
        <attr name="tl_indicatorWithoutPadding" format="boolean"/>
        <attr name="tl_indicatorInFront" format="boolean"/>
        <attr name="tl_indicatorInterpolation" format="enum">
            <enum name="smart" value="0"/>
            <enum name="linear" value="1"/>
        </attr>
        <attr name="tl_indicatorGravity" format="enum">
            <enum name="bottom" value="0"/>
            <enum name="top" value="1"/>
            <enum name="center" value="2"/>
        </attr>
        <attr name="tl_indicatorColor" format="color"/>
        <attr name="tl_indicatorColors" format="reference"/>
        <attr name="tl_indicatorThickness" format="dimension"/>
        <attr name="tl_indicatorWidth" format="dimension">
            <enum name="auto" value="-1"/>
        </attr>
        <attr name="tl_indicatorCornerRadius" format="dimension"/>
        <attr name="tl_overlineColor" format="color"/>
        <attr name="tl_overlineThickness" format="dimension"/>
        <attr name="tl_underlineColor" format="color"/>
        <attr name="tl_underlineThickness" format="dimension"/>
        <attr name="tl_dividerColor" format="color"/>
        <attr name="tl_dividerColors" format="reference"/>
        <attr name="tl_dividerThickness" format="dimension"/>
        <attr name="tl_defaultTabBackground" format="reference"/>
        <attr name="tl_defaultTabTextAllCaps" format="boolean"/>
        <attr name="tl_defaultTabTextColor" format="color|reference"/>
        <attr name="tl_defaultTabTextSize" format="dimension"/>
        <attr name="tl_defaultTabTextHorizontalPadding" format="dimension"/>
        <attr name="tl_defaultTabTextMinWidth" format="dimension"/>
        <attr name="tl_customTabTextLayoutId" format="reference"/>
        <attr name="tl_customTabTextViewId" format="reference"/>
        <attr name="tl_distributeEvenly" format="boolean"/>
        <attr name="tl_clickable" format="boolean"/>
        <attr name="tl_titleOffset" format="dimension">
            <enum name="auto_center" value="-1"/>
        </attr>
        <attr name="tl_drawDecorationAfterTab" format="boolean"/>
    </declare-styleable>

</resources>
